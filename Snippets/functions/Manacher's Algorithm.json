      "body": [
        "vector<int> manacher_odd(string s) {",
        "    int n = s.size();",
        "    s = \"$\" + s + \"^\";",
        "    vector<int> p(n + 2);",
        "    int l = 1, r = 1;",
        "    for(int i = 1; i <= n; i++) {",
        "        p[i] = max(0, min(r - i, p[l + (r - i)]));",
        "        while(s[i - p[i]] == s[i + p[i]]) {",
        "            p[i]++;",
        "        }",
        "        if(i + p[i] > r) {",
        "            l = i - p[i], r = i + p[i];",
        "        }",
        "    }",
        "    vector<int> v(begin(p) + 1, end(p) - 1);",
        "    vector<int> ans;",
        "    for(auto a:v){",
        "        ans.push_back(2*a-1);",
        "    }",
        "    return ans;",
        "}",
        "vector<int> manacher(string s) {",
        "    string t;",
        "    for(auto c: s) {",
        "        t += string(\"#\") + c;",
        "    }",
        "    auto res = manacher_odd(t + \"#\");",
        "    vector<int> v(begin(res) + 1, end(res) - 1);",
        "    vector<int> ans;",
        "    for(int i=0;i<v.size();i+=2){",
        "        ans.push_back((v[i]-1)/2);",
        "    }",
        "    return ans;",
        "}",
        ""
